
./kv.c: ../mechanisms//kv.mod
	@printf " -> $(C_GREEN)NMODL$(C_RESET) $<\\n"
	(cd "../mechanisms"; MODLUNIT=$(NRNUNITS) $(NOCMODL) kv.mod -o "/ems/elsc-labs/segev-i/moria.fridman/project/data_analysis_git/data_analysis/test_moo/x86_64")

./kv.o: ./kv.c
	@printf " -> $(C_GREEN)Compiling$(C_RESET) $<\\n"
	$(COMPILE) -I"../mechanisms" $(INCLUDES) -fPIC -c $< -o $@

